{
    "version": "2.0.0",
    "tasks": [
      {
        "type": "npm",
        "script": "dev",
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "isBackground": true,
        "problemMatcher": {
          "base": "$tsc-watch",
          "background": {
            "beginsPattern": "^[\\s]*Starting development server",
            "endsPattern": "^[\\s]*Compiled successfully"
          }
        },
        "label": "npm: dev",
        "options": {
          "cwd": "${workspaceFolder}/dashboard"
        }
      }
    ]
  }